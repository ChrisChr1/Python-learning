data = open("rosalind_iev.txt")


expect = 0
for i in data:
    for i, word in enumerate(i.split()):
        word = int(word)
        #print(expect, word)
        if i <= 2:
            expect += 2*word
            
        if i == 3:
            expect += 2*(0.75*word)
            
        if i ==4:
            expect += 2*(0.5*word)
           
        if i ==5:
            break
print(expect)
